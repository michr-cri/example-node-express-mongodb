    "mongoose": "^4.9.0",
    "mongoose-auto-increment": "^5.0.1",
    "mongoose-unique-validator": "^1.0.5",
    "morgan": "^1.8.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "mongodb-download": "^2.1.6",
    "sinon": "^2.0.0"
  }
