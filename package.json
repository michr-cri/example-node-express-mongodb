{
  "name": "example-node-express-mongodb",
  "version": "1.0.0",
  "description": "The example project that returns manages json documents persistence in a mongo db using express framework.",
  "main": "server.js",
  "scripts": {
    "startMongoDB": "bash ./build-scripts/start-mongodb.sh",
    "start:local": "npm run startMongoDB && node server.js",
